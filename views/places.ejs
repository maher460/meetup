 <div id="section1" class="sections">

 </br>

    <h2 class="text-center">Places</h2>

        </br></br>

    <!-- Panel 1   --> 
    <div class="row">
        <div id="my_panel1" class="large-8 small-centered panel text-center">


          </br>
          <div class="row">
              <h5>Group Key:</h5>
              <h3 id="group_key"> <%= group_key %> </h3>
          </div>
         

          </br>
          <div class="row">
              <h5>Members who did not vote yet:</h5>
              <div id="not_voted_members"></div>
          </div>

          </br>
          </br>
          <h2>Vote:</h2>

           <% for(var i=0; i < obj.length; i++) { %>

          <center><div class = "row panel callout radius">
              <img src= <%= obj[i].icon %> width="50" height="50">
              <h4>Rating: <%= obj[i].rating %></h4>
              <h3><%= obj[i].name %></h3>
              <h4><%= obj[i].vicinity %></h4>
              <button class="vote" value= <%= obj[i].place_id %> > Vote!</button>
          </div></center>

          <% } %>

         
        </div>



    </div>


 </div>
  
